module huawei-ac-nes {
  namespace "urn:huawei:yang:huawei-ac-nes";
  prefix "inv";
  
  import ietf-yang-types { prefix yang;}  
  
   organization "Huawei Technologies Co., Ltd.";
    contact "Huawei Industrial Base
    Bantian, Longgang
    Shenzhen 518129
    People's Republic of China
    Website: http://www.huawei.com
    Email: support@huawei.com";
						
  revision 2016-03-04 {
    description      
	"Initial revision.";
  } 
  
  identity ne-context {
        description "A ne-context is a classifier for node elements which allows an RPC to provide a service on behalf of a particular element in the data tree.";
  }

  grouping ne-context-ref {
        description
        "A helper grouping which contains a reference to a node classified with a node-context. This allows RPCs in other yang files to refine their input to a particular node instance.";

        leaf ne {
            //ext:context-reference "ne-context";
            type instance-identifier;
            description "A reference to a particular node.";
        }
    }
     container devices {
            list device {
                key deviceid;
                leaf deviceid {
                    type string;
                }
	
            container inventory-cfg {
                  description "";
          	    container nes {
          	    description "";

                  list ne {
          		    //hw-ext:detach true;
                      key "ne-id";
                      description "nodes information in the topology";
                      //ext:context-instance "ne-context";
                      uses ne;        
                         
                }  
              }
          	
            }
            

            container inventory-oper {
          	    description "";
          	    config false;
              
          	    container nes {
          		    list ne {       
          			//hw-ext:detach true;
          			key "ne-id";
                      description "nodes information in the topology";
                      //ext:context-instance "ne-context";
                      uses ne; 
          			      
          		  }  
          		}
                
            }
             
            
            typedef uuid {
          	  type yang:uuid;
                description "uuid of NE";
          	}

            grouping ne{
              description "Grouping of ne configuration.";
              
              leaf ne-id {
                type uuid;      
              }
          	
            }

          }
      }
  
}