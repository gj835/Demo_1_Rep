    prefix "sync-ext";

    import huawei-ac-ne-pub-type {
        prefix huawei-pub-type;
    }

    
    organization "Huawei Technologies Co., Ltd.";
    contact "Huawei Industrial Base
    Bantian, Longgang
    Shenzhen 518129
    People's Republic of China
    Website: http://www.huawei.com
    Email: support@huawei.com";
    description "ACV300R001C00 Yang";
    revision 2016-01-06 {
        description
            "Initial version";
